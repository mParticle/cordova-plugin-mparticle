{
  "name": "cordova-plugin-mparticle",
  "version": "2.2.4",
  "description": "Cordova plugin for mParticle",
  "homepage": "https://www.mparticle.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/mParticle/cordova-plugin-mparticle",
    "directory": "plugin"
  },
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  },
  "main": "www/mparticle.js",
  "scripts": {
    "test": "npm run-script eslint",
    "eslint": "node node_modules/eslint/bin/eslint www && node node_modules/eslint/bin/eslint src && node node_modules/eslint/bin/eslint tests",
    "fix": "./node_modules/standard/bin/cmd.js --fix",
    "prepublishOnly": "cp ../README.md README.md"
  },
  "standard": {
    "ignore": [
      "/mparticle-example"
    ]
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "standard": "^17.1.2"
  },
  "cordova": {
    "id": "cordova-plugin-mparticle",
    "platforms": [
      "android",
      "ios"
    ]
  }
}

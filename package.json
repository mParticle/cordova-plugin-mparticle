{
  "name": "cordova-plugin-mparticle",
  "version": "1.0.6",
  "description": "Cordova plugin for mParticle",
  "homepage": "https://www.mparticle.com",
  "repository": "mParticle/cordova-plugin-mparticle",
  "license": "Apache-2.0",
  "main": "www/mparticle.js",
  "scripts": {
    "test": "./node_modules/standard/bin/cmd.js && ./gradlew :src:android:test && cd mparticle-example/platforms/ios && pod install && xcodebuild -workspace MParticleExample.xcworkspace -scheme MParticleExample",
    "fix": "./node_modules/standard/bin/cmd.js --fix"
  },
  "standard": {
    "ignore": [
      "/mparticle-example"
    ]
  },
  "devDependencies": {
    "standard": "^8.6.0"
  },
  "cordova": {
    "id": "cordova-plugin-mparticle",
    "platforms": [
      "android",
      "ios"
    ]
  }
}
